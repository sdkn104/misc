   'doctop': 20188.683283,
   'height': 15.986999999999966,
   'object_type': 'textboxhorizontal',
   'page_number': 38,
   'text': '②\u3000転落しないように、ベッドに体幹や四肢をひも等で縛る。\n',
   'top': 208.68328300000002,
   'width': 370.406488,
   'x0': 33.931,
   'x1': 404.337488,
   'y0': 315.329717,
   'y1': 331.316717},
