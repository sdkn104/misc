   'doctop': 8261.516297551183,
   'height': 20.012999999999977,
   'object_type': 'textboxhorizontal',
   'page_number': 15,
   'text': '「何やってるんだ！」と注意した。\n',
   'top': 297.125746370079,
   'width': 321.230078,
   'x0': 78.945,
   'x1': 400.175078,
