   'doctop': 12152.220866141739,
   'height': 23.113999999999976,
   'object_type': 'textboxhorizontal',
   'page_number': 22,
   'text': '\uf06e パワーハラスメントに気付いた人は\n',
   'top': 205.63503937007897,
   'width': 308.485,
   'x0': 58.706,
   'x1': 367.19100000000003,
   'y0': 340.136,
   'y1': 363.25},
  {'bottom': 270.583332370079,
